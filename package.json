{
  "name": "js-polynomial-regression",
  "description": "A javascript library that predicts dependent variables using polynomial regression.",
  "version": "0.9.1",
  "keywords": [
    "regression",
    "polynomial regression",
    "prediction",
    "statistics"
  ],
  "license": "MIT",
  "author": "Robert Menke",
  "repository": "https://github.com/RobertMenke/JS-Polynomial-Regression",
  "bugs": "https://github.com/RobertMenke/JS-Polynomial-Regression/issues",
  "main": "dist/PolynomialRegression.js",
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "babel-loader": "^9.2.1",
    "nvd3": "^1.8.6",
    "webpack": "^5.97.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "start:dev": "webpack --progress --watch --mode development",
    "start:example:dev": "webpack --progress --watch --config weback.example.config.js --mode development",
    "build": "webpack --progress --mode production",
    "build:example": "webpack --progress --config weback.example.config.js --mode production",
    "build:all": "npm run build && npm run build:example"
  }
}
